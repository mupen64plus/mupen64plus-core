DECLARE_INSTRUCTION(LWC1)
{
   const unsigned char lslfft = lfft;
   const uint32_t lslfaddr = (uint32_t) reg[lfbase] + lfoffset;
   uint64_t temp;
   if (check_cop1_unusable()) return;
   ADD_TO_PC(1);
   address = lslfaddr;
   rdword = &temp;
   read_word_in_memory();
   if (address)
     *((int*)reg_cop1_simple[lslfft]) = (int) *rdword;
}

